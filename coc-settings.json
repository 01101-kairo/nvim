{
    "coc.preferences.formatOnSaveFiletypes": [
        "*"
    ],
    "suggest.languageSourcePriority": 99,
    "suggest.timeout": 500,
    "suggest.snippetIndicator": " \ue796",
    "suggest.completionItemKindLabels": {
        "method": "  ",
        "function": "  ",
        "variable": "[]",
        "field": "  ",
        "struct": "  ",
        "event": "  ",
        "property": "  ",
        "reference": "  ",
        "file": "  ",
        "color": "  ",
        "constructor": "  ",
        "typeParameter": "<>",
        "class": "\uf0e8",
        "snippet": "\ue60b",
        "interface": " 蘒",
        "enum": " 練",
        "unit": " 塞 ",
        "folder": " ﱮ ",
        "text": "m  ",
        "enumMember": "  ",
        "constant": "y  ",
        "operator": "j  ",
        "module": "k  ",
        "keyword": "l  ",
        "value": "q  "
    },
    "diagnostic.displayByAle": true,
    // coc-explorer
    "explorer.width": 40,
    "explorer.file.column.filename.width": 20,
    "explorer.file.showHiddenFiles": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.sources": [
        {
            "name": "file",
            "expand": true
        }
    ],
    "explorer.file.columns": [
        "indent",
        "selection",
        "icon",
        "filename",
        "size",
        "readonly"
    ],
    "explorer.keyMappings": {
        "<tab>": "toggleSelection",
    },
    "explorer.icon.expanded": " ",
    "explorer.icon.collapsed": " ",
    "explorer.icon.selected": "",
    "explorer.file.column.git.icon.unmodified": "  ",
    "explorer.file.column.git.icon.mixed": "* ",
    "explorer.file.column.git.icon.modified": " ",
    "explorer.file.column.git.icon.untracked": " ",
    "explorer.file.column.git.icon.deleted": " ",
    "explorer.file.column.git.icon.added": " ",
    // coc-emmet
    "emmet.includeLanguages": {
        "vue-html": "html",
        "javascript": "javascriptreact"
    },
    // coc-python
    "python.pythonPath": "/usr/bin/python3",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--ignore=E501,E402,F401,E701"
    ],
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Args": [
        "--ignore=E501"
    ],
    // coc-yaml
    "yaml.validate": true,
    "yaml.format.enable": true,
    "yaml.completion": true,
    "prettier.formatterPriority": -1,
    "prettier.printWidth": 0
}
